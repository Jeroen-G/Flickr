{
    "name": "jeroen-g/flickr",
    "description": "Modern PHP package to make Flickr API calls. Ships with Laravel implementation.",
    "keywords": [
        "JeroenG",
        "Flickr",
        "API",
        "PHP",
        "Laravel"
    ],
    "homepage": "https://github.com/Jeroen-G/Flickr",
    "license": "EUPL-1.1",
    "authors": [
        {
            "name": "JeroenG",
            "email": "jeroengjeroeng@gmail.com",
            "homepage": "https://github.com/Jeroen-G"
        }
    ],
    "require": {
        "guzzlehttp/guzzle" : "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~6.0"
    },
    "suggest": {
        "illuminate/support": "This package provides a Laravel Service Provider"
    },
    "autoload": {
        "psr-4": {
            "JeroenG\\Flickr\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JeroenG\\Flickr\\FlickrServiceProvider"
            ],
            "aliases": {
                "Activity": "JeroenG\\Flickr\\FlickrLaravelFacade"
            }
        }
    }
}
